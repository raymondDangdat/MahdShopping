<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="comq.example.raymond.mahdshopping">

    <application
        android:allowBackup="true"
        android:icon="@drawable/reallogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/reallogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name=".Register"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AdminHome"
            android:label="@string/title_activity_admin_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".UsersHome"
            android:label="@string/title_activity_users_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddFrameActivity"
            android:parentActivityName=".AdminHome" />
        <activity
            android:name=".CustomerOrder"
            android:parentActivityName=".UsersHome" />
        <activity
            android:name=".DisplayFrames"
            android:parentActivityName=".UsersHome" />
        <activity
            android:name=".AdminViewOrders"
            android:parentActivityName=".AdminHome" />
        <activity
            android:name=".ContactCustomerCare"
            android:parentActivityName=".UsersHome" />
        <activity
            android:name=".CustomerFeedBack"
            android:parentActivityName=".AdminHome" />
        <activity android:name=".ReplyCustomerFeedBack"
            android:parentActivityName=".CustomerFeedBack"/>
    </application>

</manifest>